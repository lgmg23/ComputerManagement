@model IEnumerable<COMPUTERMANAGEMENT_DAL.t_Tipo>

@{
    ViewBag.Title = "Lista de tipos";
}

<h2>Lista de tipos</h2>

<p>
    @Html.ActionLink("Agregar tipo", "AgregarTipo")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tipo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo)
        </td>
    </tr>
}

</table>
