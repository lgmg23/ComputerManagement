@model IEnumerable<COMPUTERMANAGEMENT_DAL.t_Marca>

@{
    ViewBag.Title = "Lista de marcas";
}

<h2>Lista de marcas disponibles</h2>

<p>
    @Html.ActionLink("Agregar marca nuevo", "Marca")
</p>
<table class="table">
    <tr>        
        <th>
            @Html.DisplayNameFor(model => model.Marca)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>        
        <td>
            @Html.DisplayFor(modelItem => item.Marca)
        </td>
    </tr>
}

</table>
