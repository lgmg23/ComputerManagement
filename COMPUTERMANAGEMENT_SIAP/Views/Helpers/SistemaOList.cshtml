@model IEnumerable<COMPUTERMANAGEMENT_DAL.t_SistemaO>

@{
    ViewBag.Title = "SistemaOList";
}

<h2>Lista de sistemas operativos disponibles</h2>

<p>
    @Html.ActionLink("Agregar sistema operativo", "SistemaO")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SistemaO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SistemaO)
        </td>
    </tr>
}

</table>
