@model IEnumerable<COMPUTERMANAGEMENT_DAL.t_Marca>

@{
    ViewBag.Title = "MarcaList";
}

<h2>Lista de marcas</h2>

<p>
    @Html.ActionLink("Agregar marca", "AgregarMarca")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Marca)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Marca)
        </td>
    </tr>
}

</table>
