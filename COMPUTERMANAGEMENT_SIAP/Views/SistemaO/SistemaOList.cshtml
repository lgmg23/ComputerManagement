@model IEnumerable<COMPUTERMANAGEMENT_DAL.t_SistemaO>

@{
    ViewBag.Title = "Lista de sistemas operativos";
}

<h2>Lista de sistemas operativos</h2>

<p>
    @Html.ActionLink("Agregar Sistema Operativo", "AgregarSistemaO")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SistemaO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SistemaO)
        </td>
    </tr>
}

</table>
